{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f ./env/.env.local next dev",
    "start": "next start",
    "lint": "tsc --noEmit && next lint --dir .",
    "build:local": "env-cmd -f ./env/.env.local next build",
    "build:development": "env-cmd -f ./env/.env.development yarn typechain && next build",
    "generate": "graphql-codegen --config codegen.yml",
    "prestart": "yarn generate",
    "predev": "yarn generate"
  },
  "dependencies": {
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@rainbow-me/rainbowkit": "^0.4.2",
    "@tanstack/react-query": "^4.0.10",
    "antd": "^4.21.5",
    "ethers": "^5.6.9",
    "graphql": "^16.5.0",
    "graphql-request": "^4.3.0",
    "ipfs-http-client": "^57.0.3",
    "it-to-browser-readablestream": "^1.0.4",
    "it-to-buffer": "^2.0.2",
    "jsbarcode": "^3.11.5",
    "magic-bytes.js": "^1.0.12",
    "next": "12.2.0",
    "qrcode.react": "^3.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-pdf": "^5.7.2",
    "uuid": "^8.3.2",
    "wagmi": "^0.5.9"
